        "@gtile/gcr-4": "4.1.0-3.2.3",
        "@gtile/gio-2.0": "2.78.0-3.2.3",
        "@gtile/gjs": "3.2.3",
        "@gtile/glib-2.0": "2.78.0-3.2.3",
        "@gtile/gnomebg-4.0": "4.0.0-3.2.3",
